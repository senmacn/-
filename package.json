{
  "name": "mapeditor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "map-editor for pwrd-7",
  "main": "electron/main/dist/index.cjs",
  "scripts": {
    "start": "vite --port=4000",
    "dev": "vite --port=4000",
    "build": "vite build",
    "build:electron": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./electron/main && vite build",
    "build:preload": "cd ./electron/preload && vite build",
    "build:renderer": "vite build",
    "compile": "cross-env MODE=production npm run build:electron && electron-builder build --config scripts/electron-builder.config.cjs --dir --config.asar=false",
    "watch": "node scripts/watch.mjs",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue",
    "preview": "vite preview",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "typecheck:main": "tsc --noEmit -p tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p electron/preload/tsconfig.json",
    "typecheck:renderer": "vue-tsc --noEmit -p electron/renderer/tsconfig.json"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.44.1",
    "@scena/ruler": "^0.18.3",
    "@simonwep/pickr": "^1.8.2",
    "@vue/runtime-core": "^3.2.47",
    "@vue/shared": "^3.2.47",
    "@vueuse/core": "^9.13.0",
    "cross-env": "7.0.3",
    "d3-quadtree": "^3.0.1",
    "lodash-es": "^4.17.21",
    "moveable": "^0.46.1",
    "pinia": "^2.0.33",
    "vue": "^3.2.45",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/d3-quadtree": "^3.0.2",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.10",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/runtime-core": "^3.2.47",
    "@vue/shared": "^3.2.47",
    "consola": "^2.15.3",
    "electron": "23.2.1",
    "electron-builder": "23.6.0",
    "electron-updater": "5.3.0",
    "eslint": "8.37.0",
    "eslint-plugin-vue": "9.10.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "typescript": "^4.9.3",
    "unplugin-auto-expose": "0.0.4",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.0",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^1.0.24"
  }
}